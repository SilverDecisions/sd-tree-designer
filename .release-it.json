{
  "non-interactive": false,

  "increment": false,
  "preReleaseId": null,
  "hooks": {
    "after:bump": ["ncu -u --packageFile package.json sd-utils sd-model", "npm update sd-utils sd-model", "gulp"]
  },
  "changelogCommand": "git log --pretty=format:\"* %s (%h)\" [REV_RANGE]",
  "npm": {
    "publish": true
  },
  "git": {
    "requireCleanWorkingDir": false
  }
}
