  ],
  "browserify": {
    "transform": [
      [
        "stringify",
        {
          "appliesTo": {
            "includeExtensions": [
              ".html"
            ]
          }
        }
      ],
      [
        "babelify",
        {
          "global": true,
          "ignore": [
            "/\\/node_modules\\/(?!d3-.+\\/)/"
          ],
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "transform-class-properties",
            "transform-object-assign",
            [
              "babel-plugin-transform-builtin-extend",
              {
                "globals": [
                  "Error"
                ]
              }
            ],
            [
              "@babel/plugin-transform-runtime",
              {
                "regenerator": true
              }
            ]
          ]
        }
      ]
    ]
  },
  "devDependencies": {
